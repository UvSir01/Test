package com.collection.byme;

import java.util.ArrayList;

public class Test {
public static void main(String[] args) {
	ArrayList al=new ArrayList();
	al.add(10);
	al.add(null);
	al.add(new Employee(101, "durga"));
	al.add(new Student(102, "juned"));
	//System.out.println("The AL is: "+al.get(2));
	Object o=al.get(2);
	if(o instanceof Student)
	{
		Student s=(Student)o;
		System.out.println(s.rollNo+" "+s.name);
	}
	
		
	
}
}
