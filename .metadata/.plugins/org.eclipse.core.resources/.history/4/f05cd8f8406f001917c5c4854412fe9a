package com.collection.byme;

import java.util.ArrayList;

public class Test {
public static void main(String[] args) {
	//type safety using generics
	ArrayList al=new ArrayList();
al.add(10);
al.add(11);
al.add("uvesh");
al.add(10.5);
al.add("java");
System.out.println(al.isEmpty());
al.set(0, "Javed");
System.out.println(al);
al.remove(0);
System.out.println(al);
}
}
