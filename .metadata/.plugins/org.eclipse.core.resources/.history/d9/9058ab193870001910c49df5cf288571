package com.collection.byme;

import java.util.ArrayList;

public class Test {
public static void main(String[] args) {
	//type safety using generics
	ArrayList al=new ArrayList();
al.add(new Employee(101, "uves"));
al.add(new Employee(102, "juned"));
ArrayList al2=new ArrayList();
al2.addAll(al);
//al2.add(new );
//al2.add(10.5);
//al.add("java");
